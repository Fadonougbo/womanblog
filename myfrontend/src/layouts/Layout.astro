---
import "../styles/global.css"


interface Props {
	title?:string
}

const {title}=Astro.props
---
<!doctype html>
<html lang="fr">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<title>{title??'Woman Blog'}</title>
	</head>
	<body class="bg-white" >
		<slot />
	</body>
</html>

